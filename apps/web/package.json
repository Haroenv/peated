{
  "private": true,
  "name": "web",
  "scripts": {
    "assets": "npx @svgr/cli --out-dir app/components/assets -- app/assets",
    "assets:watch": "npm-watch assets",
    "build": "run-s build:*",
    "build:remix": "cross-env NODE_ENV=production remix build --sourcemap",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:assets": "run-s assets",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec 'esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle && node --require dotenv/config ./build/server.js' --watch ./server.ts",
    "dev:assets": "run-s assets assets:watch",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/raleway": "^5.0.8",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@peated/design": "workspace:*",
    "@peated/shared": "workspace:*",
    "@peated/tsconfig": "workspace:*",
    "@react-oauth/google": "^0.11.1",
    "@remix-run/css-bundle": "^1.19.3",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/express": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@sentry/react": "^7.69.0",
    "@sentry/remix": "^7.69.0",
    "@sentry/vite-plugin": "^2.7.1",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^4.35.0",
    "@types/compression": "^1.7.3",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.17",
    "@types/leaflet": "^1.9.4",
    "@types/morgan": "^1.9.5",
    "@types/pica": "^9.0.1",
    "@types/react": "^18.2.21",
    "@types/react-avatar-editor": "^13.0.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.15",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "d3": "^7.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.20",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "isbot": "latest",
    "leaflet": "^1.9.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "pica": "^9.0.1",
    "postcss": "^8.4.29",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-dom": "^18.2.0",
