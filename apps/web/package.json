    "tailwindcss": "^3.3.3",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.2.2",
    "use-dehydrated-state": "^0.1.0",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.2"
  },
  "proxy": "http://localhost:4000",
  "watch": {
    "assets": {
      "patterns": [
        "assets"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
