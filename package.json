{
  "private": true,
  "name": "cask",
  "version": "0.1.0",
  "description": "",
  "workspaces": [
    "web",
    "scraper",
    "server"
  ],
  "main": "main.js",
  "scripts": {
    "dev": "nf start",
    "dev:web": "API_SERVER=http://localhost:4000 dotenv -e ./.env -- npm run dev -w web",
    "dev:server": "CORS_HOST=http://localhost:5173 PORT=4000 dotenv -e ./.env -- npm run dev -w server",
    "db:studio": "dotenv -e ./.env -- npx -w server prisma studio",
    "db:migrate": "dotenv -e ./.env -- npm run db:migrate -w server",
    "test:server": "dotenv -e ./.env -- npm run test -w server",
    "token": "dotenv -e ./.env -- npm run token -w server"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "foreman": "^3.0.1",
    "vite": "^4.2.1"
  }
}
